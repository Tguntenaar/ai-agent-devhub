                      "DevDAO Platform",
                      "Events & Hackathons",
                      "Engagement & Awareness",
                      "Decentralized DevRel",
                      "Universities & Bootcamps",
                      "Tooling & Infrastructure",
                      "Other"
                    ]
                  },
                  "summary": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "linked_proposals": {
                    "type": "array",
                    "items": {
                      "type": "number"
                    }
                  },
                  "labels": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "amount": {
                    "type": "number"
                  },
                  "currency": {
                    "type": "string"
                  },
                  "receiver_account": {
                    "type": "string"
                  },
                  "requested_sponsor": {
                    "type": "string"
                  },
                  "supervisor": {
                    "type": "string"
                  },
                  "timeline": {
                    "type": "string",
                    "enum": ["DRAFT", "REVIEW"]
                  },
                  "linked_rfp": {
                    "type": "number"
                  },
                  "accepted_terms": {
                    "type": "boolean"
                  }
                },
                "required": ["title", "category", "summary", "description", "linked_proposals", "labels", "amount", "currency", "receiver_account", "requested_sponsor", "supervisor", "timeline"]
              }
            }
          }
        },
        "responses": {
          "200": {
